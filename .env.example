# Environment Variables
# Copy this file to .env.local and update the values

# Email Service Configuration (for form submissions)
EMAIL_SERVICE_API_KEY=your_email_service_api_key
EMAIL_FROM=noreply@ecobiogas.com
EMAIL_TO=info@ecobiogas.com

# Database Configuration (if using a database)
DATABASE_URL=your_database_url

# Google Maps API Key (for map integration)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# WhatsApp Business API
WHATSAPP_API_URL=your_whatsapp_api_url
WHATSAPP_PHONE_NUMBER=+15551234567

# Analytics (optional)
NEXT_PUBLIC_GA_ID=your_google_analytics_id

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://ecobiogas.com
NEXT_PUBLIC_SITE_NAME=EcoBiogas
